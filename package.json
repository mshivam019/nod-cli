{
  "name": "nod-cli",
  "version": "0.1.0",
  "description": "Backend scaffolding CLI for Express, Hono, and more - with best practices built-in",
  "main": "./dist/index.js",
  "type": "module",
  "bin": {
    "nod-cli": "./dist/cli.js",
    "nod": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "cli",
    "backend",
    "scaffolding",
    "express",
    "hono",
    "typescript",
    "nodejs",
    "api",
    "rest",
    "jwt",
    "database",
    "middleware",
    "create",
    "generator",
    "boilerplate",
    "route-builder",
    "nod-cli"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nod-cli.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nod-cli/issues"
  },
  "homepage": "https://github.com/yourusername/nod-cli#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "prompts": "^2.4.2",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  }
}
